    <div class="header">
        <div class="container">
            <div class="row">
                <div class="col-xl-3 col-lg-3 col-md-3 col-sm-3 col logo_section">
                    <div class="full">
                        <div class="center-desk">
                            <div class="logo"> <a href="/"><img src="/img/logo.jpg" alt="#"></a> </div>
                        </div>
                    </div>
                </div>
                <div class="col-xl-9 col-lg-9 col-md-9 col-sm-9">
                    <div class="menu-area">
                        <div class="limit-box">
                            <nav class="main-menu">
                                <ul class="menu-area-main">

                                    {{#user}}
                                        <li> <a href="#">{{name}}님 안녕하세요 </a> </li>
                                    {{/user}}

                                    {{#avatar}}
                                        {{#user}}
                                        <li class="active"> <a href="/">Home</a> </li>
                                        <li> <a href="/avatar/create/{{id}}">Character </a> </li>
                                        <li> <a href="/exercise/start">Start </a> </li>
                                        <li> <a href="contact.html">Ranking </a> </li>
                                        <li> <a href="/logout">Logout </a> </li>
                                        {{/user}}
                                    {{/avatar}}

                                    {{^avatar}}
