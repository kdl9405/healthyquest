                                            <li> <a href="/oauth2/authorization/google">Google Login </a> </li>

                                        {{/user}}
                                    {{/avatar}}


                                    <li class="mean-last"> <a href="#"><img src="/img/search_icon.png" alt="#" /></a> </li>
                                    <li class="mean-last"> <a href="#"><img src="/img/top-icon.png" alt="#" /></a> </li>
                                </ul>
                            </nav>
                        </div>
                    </div>
                </div>
            </div>
